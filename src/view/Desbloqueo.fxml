<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="45.0" prefWidth="340.0" stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.BloqueoController">
   <children>
      <HBox prefHeight="45.0" prefWidth="360.0" style="-fx-background-color: rgb(50,50,50);">
         <children>
            <TextField fx:id="desbloqueoTxt" prefHeight="27.0" prefWidth="315.0" promptText="Introduce the unlocking word" style="-fx-background-color: rgb(50,50,50); -fx-border-color: white;" />
            <Button mnemonicParsing="false" onAction="#handleContinuar" prefHeight="25.0" prefWidth="49.0" text="Ok" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
